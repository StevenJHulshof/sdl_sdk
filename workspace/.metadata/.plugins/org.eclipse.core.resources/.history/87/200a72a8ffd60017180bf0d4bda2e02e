#include "Menu.h"

Menu::Menu(int xPos, int yPos, int type):
	GameObject()
{
	setTextureType();
}

Menu::~Menu()
{

}

void Menu::setTextureType()
{
	switch(_type)
	{
	case TYPE_OBJECT_MENU_ACTION:
		_textureType = TEXTURE_OBJECT_MENU_ACTION;
		break;
	case TYPE_OBJECT_MENU_STAT:
		_textureType = TEXTURE_OBJECT_MENU_ACTION;
		break;
	default:
		break;
	}
}
