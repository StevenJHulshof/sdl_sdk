#include "menu_create.h"

void createActionMenu(int xPos, int yPos)
{
	ActionMenu *actionMenu = new ActionMenu(xPos, yPos);
	gGlobalObjectPool._warriorPool.push_back(actionMenu);
}

void createStatMenu(int xPos, int yPos)
{
	StatMenu *statMenu = new StatMenu(xPos, yPos);
	gGlobalObjectPool._warriorPool.push_back(statMenu);
}
