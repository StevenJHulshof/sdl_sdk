#include "Grid.h"

int Grid::_tileGrid[GRID_X][GRID_Y] = 0;
int Grid::_playableGrid[GRID_X][GRID_Y] = 0;

Grid::Grid()
{

}

Grid::~Grid()
{

}

int *Grid::getTileGrid()
{
	return _tileGrid;
}

int *Grid::getPlayable()
{
	return _playableGrid;
}
